{
  "expo": {
    "name": "reconnaissance-automation",
    "slug": "reconnaissance-automation",
    "scheme": "myapp",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icons/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.reconnaissanceautomation"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.anonymous.reconnaissanceautomation"
    },
    "web": {
      "favicon": "./assets/icons/favicon.png"
    },
    "plugins": ["expo-secure-store"]
  }
}
