{
  "name": "reconnaissance-automation",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "keywords": [
    "reconnaissance",
    "optimization",
    "automation",
    "report generation",
    "react",
    "react-native",
    "typescript",
    "expo",
    "zustand",
    "formik",
    "zod",
    "tamagui",
    "react-navigation"
  ],
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios:dev": "open -a Simulator.app && expo run:ios",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "pod": "cd ios && pod install && cd ..",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "check-all": "npm run lint && npm run format && npm run type-check",
    "prepare": "husky"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:fix",
      "npm run format"
    ]
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.1",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@shopify/flash-list": "1.6.4",
    "@tamagui/babel-plugin": "^1.96.0",
    "@tamagui/config": "^1.96.0",
    "@tamagui/lucide-icons": "^1.96.0",
    "@xsolla/metrika": "^1.6.9",
    "axios": "^1.6.8",
    "expo": "~51.0.2",
    "expo-blur": "~13.0.2",
    "expo-file-system": "~17.0.1",
    "expo-font": "~12.0.4",
    "expo-image": "~1.12.9",
    "expo-image-picker": "~15.0.5",
    "expo-linking": "~6.3.1",
    "expo-location": "~17.0.1",
    "expo-secure-store": "~13.0.1",
    "expo-sharing": "~12.0.1",
    "expo-splash-screen": "~0.27.4",
    "expo-status-bar": "~1.12.1",
    "formik": "^2.4.6",
    "immer": "^10.1.1",
    "react": "18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "0.74.1",
    "react-native-calendars": "^1.1302.0",
    "react-native-maps": "1.14.0",
    "react-native-mmkv": "^2.12.2",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "~3.31.1",
    "react-native-svg": "15.2.0",
    "tamagui": "^1.96.0",
    "use-immer": "^0.9.0",
    "zod": "^3.23.6",
    "zod-formik-adapter": "^1.3.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.3.1",
    "eslint": "^8.56.0",
    "eslint-config-expo": "^7.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.3.0"
  },
  "private": true
}
